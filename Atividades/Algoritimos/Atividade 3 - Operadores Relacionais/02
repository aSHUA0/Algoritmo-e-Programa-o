#include <stdio.h>
#include <locale.h>
#include <math.h>

int main(){
    
    setlocale(LC_ALL, "portuguese");
    /* atividade 03*/
    float salario;

    printf("Digite seu salario:");
    scanf("\n %f", &salario);

    if(salario >= 1320){
        printf("rico");
    }else {
        printf("pobre");
    }

    return 0;
}
