#include<stdio.h>
#include<math.h>

int main(){
  int A[10], contadorNegativo = 0, totalPositivo = 0;

  printf("Digite 10 numeros\n");
    
  for(int i = 0; i<10; i++){
    scanf("%d", &A[i]);
  }
  for(int i = 0; i<10; i++){
    if(A[i] < 0){
      contadorNegativo ++;
    }else{
      totalPositivo += A[i];
    }
  }
  printf("\nQuantidade numeros negativos: %d", contadorNegativo);
  printf("\nSoma dos numeros positivos: %d", totalPositivo);
}
