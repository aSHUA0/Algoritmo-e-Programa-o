#include<stdio.h>
#include<math.h>

int main(){
  int A[15], total = 0, media;

  printf("Digite as notas dos alunos\n");
    
  for(int i = 0; i<15; i++){
    scanf("%d", &A[i]);
  }
  for(int i = 0; i<15; i++){
    total += A[i];
  }  
  media = total / 15;
  printf("A média é %d", media);
}
