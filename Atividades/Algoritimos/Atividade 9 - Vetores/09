#include<stdio.h>
#include<math.h>

int main(){
  int A[6], n;

  do{ 
    n = 0;
    printf("Digite 6 valores inteiros pares\n");
    
    for(int i = 0; i<6; i++){
      scanf("%d", &A[i]);
    }

    for(int i = 0; i<6; i++){
      if(A[i] % 2 != 0){
        printf("O valor não é par\n");
        n = 1;
      }
    }
  }while(n != 0);

  for(int i = 5; i >=0; i--){
    printf("%d", A[i]);
  }
}
