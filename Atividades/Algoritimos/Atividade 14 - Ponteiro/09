#include <stdio.h>
#include <stdlib.h>

int main() {
  int matriz[3][3];

  for (int i = 0; i < 3; i++) {
    for (int j = 0; j < 3; j++) {
      matriz[i][j] = i * 3 + j;
    }
  }

  for (int i = 0; i < 3; i++) {
    for (int j = 0; j < 3; j++) {
      printf("EndereÃ§o de matriz[%d][%d]: %p\n", i, j, (void*)&matriz[i][j]);
    }
  }
  return 0;
}
